{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "include": ["./src/**/*","./packages/**/*"],
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@components": ["src/components"],
      "@constants": ["src/constants"],
      "@utils": ["src/utils"],
      "@assets": ["src/assets"],
      "@stores": ["src/stores"],
      "@hooks": ["src/hooks"],
      "@types": ["src/types"],
      "@models": ["src/models"],
      "@tli-up-packages/*": ["packages/tli-up-packages/*"]
    }
  }
}
