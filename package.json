{
  "name": "tli_mobile",
  "version": "0.0.1",
  "private": true,
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "start": "react-native start",
    "format": "prettier --write . --config ./.prettierrc.js",
    "format:check": "prettier --check . --config ./.prettierrc.js",
    "test": "jest",
    "commit": "npx cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@react-navigation/elements": "^2.1.5",
    "@react-navigation/native": "^7.0.6",
    "@react-navigation/native-stack": "^7.1.7",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hook-form": "^0.3.0",
    "native-base": "^3.4.28",
    "react": "18.3.1",
    "react-hook-form": "^7.53.0",
    "react-native": "0.75.4",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-reanimated": "^3.16.2",
    "react-native-safe-area-context": "^4.14.0",
    "react-native-screens": "^4.3.0",
    "react-native-svg": "12.1.1",
    "react-native-vector-icons": "^10.2.0",
    "rn-secure-storage": "^3.0.1",
    "zod": "^3.23.8",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@react-native/babel-preset": "0.75.4",
    "@react-native/eslint-config": "0.75.4",
    "@react-native/metro-config": "0.75.4",
    "@react-native/typescript-config": "0.75.4",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^9.1.6",
    "jest": "^29.6.3",
    "lint-staged": "^15.2.10",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4",
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint ./src --fix"
    ],
    "*.tsx": [
      "prettier --write",
      "eslint ./src --fix"
    ]
  }
}
